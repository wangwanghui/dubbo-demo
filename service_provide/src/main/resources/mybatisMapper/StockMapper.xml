<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.provide.dao.StockDao">


    <update id="updateStockByProId">
        UPDATE stock_tbl SET stock_num -1 WHERE  product_id =#{productId}
    </update>
</mapper>